source 'https://github.com/CocoaPods/Specs.git'
source 'https://bitbucket.org/redso/podspecs.git'
platform :ios, '13.0'
use_frameworks!


def install_pods
  pod 'Alamofire', '= 5.2.2'
  pod 'AlamofireImage', '~> 4.1'
  pod 'MXParallaxHeader'
  pod 'RxSwift', '6.0.0'
  pod 'SwiftyJSON', '~> 4.0'
end
  
target 'My-Weather-CAN' do
  install_pods
end

target 'My-Weather-AUS' do
  install_pods
end
